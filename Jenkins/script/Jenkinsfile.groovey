pipeline {
    agent any
    parameters {
         gitParameter branchFilter: 'origin/(.*)', defaultValue: 'main', name: 'BRANCH?', type: 'PT_BRANCH'
    }
    stages {
        stage('set Branch') {
            steps {
                echo '开始'
                git branch: "${params.BRANCH}", url: 'git@github.com:chdo002/DDU.git'
                echo '结束'
            }
        }
    }
}