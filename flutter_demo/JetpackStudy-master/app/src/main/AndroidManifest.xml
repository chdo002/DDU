<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.holiday.jetpackstudy">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".paging.network.PagingNetworkActivity"></activity>
        <activity android:name=".paging.network.PagingNetworkActivity2"></activity>
        <activity android:name=".room.RoomActivity"></activity>
        <activity android:name=".paging.PagingActivity"></activity>
        <activity
            android:name=".navigation.NavigationActivity"
            android:label="@string/title_activity_navigation"></activity>
        <activity android:name=".workmanager.WorkManagerActivity" />
        <activity android:name=".viewmodel_livedata.ViewModelActivity" />
        <activity android:name=".databinding.DbRecyclerViewActivity" />
        <activity android:name=".lifecycle.LifecycleActivity" />
        <activity android:name=".databinding.DataBindingActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>